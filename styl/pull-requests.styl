.github__pull-request {
  position relative;
  min-height 34px;
  padding 9px 15px 9px 50px !important;
}
.github__pull-request__avatar {
  position: absolute;
  top  9px;
  left 15px;
  display block;
  width  24px;
  height 24px;
  border-radius 100%;
  overflow hidden;
}
.github__pull-request__avatar img {
  max-width 100%;
}